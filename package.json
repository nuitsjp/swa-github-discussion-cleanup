{
  "name": "swa-github-discussion-cleanup",
  "version": "1.0.0",
  "description": "GitHub Action that deletes expired Azure Static Web Apps invite discussions.",
  "type": "module",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "format:write": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "bundle": "npm run package",
    "package": "npx rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript && npx prettier --write dist/index.js",
    "package:watch": "npm run package -- --watch"
  },
  "keywords": [
    "github-actions",
    "azure-static-web-apps",
    "swa",
    "discussions"
  ],
  "author": "Atsushi Nakamura",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.0",
    "@octokit/graphql": "^9.0.3",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "prettier": "^3.6.2",
    "rollup": "^4.53.3",
    "typescript": "^5.9.3"
  }
}
